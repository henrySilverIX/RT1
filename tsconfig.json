{
    "compilerOptions": {
      "target": "ES5",
      "module": "CommonJS",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./out",
      "rootDir": "./",
      "moduleResolution": "node",
      "noImplicitAny": true
    },
    "include": ["index.ts", "models/**/*.ts"],
    "exclude": ["node_modules"]
  }